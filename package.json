{
  "name": "dron",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "serve": "live-server .",
    "sass": "sass --watch  scss/main.scss assets/css/main.css --quiet-deps",
    "postcss": "postcss assets/css/main.css -o assets/css/main.min.css",
    "build-css": "npm run sass && npm run postcss",
    "start": "concurrently \"npm run sass\" \"npm run serve\"",
    "build": "npm run sass && npm run postcss && cp index.html dist/ && cp -R assets dist/ && cp favicon.ico dist/ && cp CNAME dist/ && cp sitemap.xml dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cssnano": "^7.0.6",
    "live-server": "^1.2.2",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-scss": "^6.10.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
